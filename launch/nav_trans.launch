<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="MOVE_TRANS" output="screen">
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/local_costmap_params_trans.yaml" command="load" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/global_costmap_params_trans.yaml" command="load" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/move_base_params.yaml" command="load" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/dwa_local_planner_params_trans.yaml" command="load" />
    <remap from="/odom" to ="/odom_lidar" />
  </node> 

  <node pkg="clear_costmap" type="clear_costmap.py" respawn="false" name="clear_costmap" output="screen" />

</launch> 
