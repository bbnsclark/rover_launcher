<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="MOVE_GLOBAL" output="screen">
    <param name="base_local_planner" value="TrajectoryPlannerROS" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/local_costmap_params_global.yaml" command="load" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/global_costmap_params_global.yaml" command="load" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/move_base_params.yaml" command="load" />
    <rosparam file="$(env HOME)/rover/src/rover_launcher/params/nav_planner_params_global.yaml" command="load" />
    <remap from="/odom" to ="/odom_inertial" />
  </node> 

  <node pkg="clear_costmap" type="clear_costmap.py" respawn="false" name="clear_costmap" output="screen" />

</launch> 
