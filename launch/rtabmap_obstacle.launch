
<launch>

      <node pkg="nodelet" type="nodelet" name="obstacles_detection" args="load rtabmap_ros/obstacles_detection stereo_nodelet">
         <remap from="cloud" to="/camera/depth/color/points"/>
         <param name="frame_id" type="string" value="base_link"/>
         <param name="map_frame_id" type="string" value="map_inertial"/>
         <param name="min_cluster_size" type="int" value="20"/>
         <param name="max_obstacles_height" type="double" value="2.0"/>

       </node>

</launch>
